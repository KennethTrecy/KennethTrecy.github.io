{
	"name": "kennethtrecy.github.io",
	"version": "v0.1.0-dev",
	"description": "This is a personal website of Kenneth Trecy Tobias.",
	"homepage": "https://github.com/KennethTrecy/kennethtrecy.github.io",
	"keywords": [
		"personal",
		"website"
	],
	"private": "true",
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"dev": "npm run development",
		"development": "npx rollup --bundleConfigAsCjs -c bundler/dev/rollup.config.js",
		"watch": "npx rollup --bundleConfigAsCjs -c bundler/dev/rollup.config.js --watch",
		"prod": "npm run production",
		"production": "npx rollup --bundleConfigAsCjs -c bundler/prod/rollup.config.js",
		"test:unit": "npx rollup -c bundler/test/rollup.config.js && npx mocha hidden/t/**/*.js",
		"test:watch_unit": "npx mocha hidden/t/**/*.js --watch --watch-files hidden/t/**/*.js"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@rollup/plugin-alias": "^4.0.2",
		"@rollup/plugin-commonjs": "^24.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.2.1",
		"@tsconfig/recommended": "^1.0.1",
		"chai": "^4.3.7",
		"changelogen": "^0.4.0",
		"esbuild": "^0.16.10",
		"husky": "^8.0.0",
		"jsdom": "^20.0.3",
		"mocha": "^10.2.0",
		"rollup": "^2.67.0",
		"rollup-plugin-esbuild-transform": "^1.5.0",
		"rollup-plugin-scss": "^4.0.0",
		"rollup-plugin-svelte": "^7.1.0",
		"sass": "^1.49.7",
		"svelte": "^3.55.0",
		"svelte-preprocess": "^5.0.0",
		"typescript": "^4.9.4"
	}
}
